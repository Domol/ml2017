    "def cals(k):\n",
    "    f_x, f_y = f(50)\n",
    "    models = []\n",
    "    for i in range(20):\n",
    "        x_train, y_train = sample(100)\n",
    "        model = fit_polynomial(x_train, y_train, k)\n",
    "        models.append(model)\n",
    "    g = lambda x: (1/float(len(models))) * sum([apply_polynomial(model,[x]) for model in models])\n",
    "    bias = 0\n",
    "    variance = 0\n",
    "    for j in range(len(f_x)):\n",
    "        bias += (g(f_x[j]) - f_y[j])**2\n",
    "        for m in range(len(models)):\n",
    "            variance += (g(f_x[j]) - apply_polynomial(models[m],[f_x[j]]))**2\n",
    "    variance /= float(len(models))\n",
    "    bias /= float(len(models))\n",
    "    mse = bias**2 + variance #????\n",
    "    return [bias, variance, mse]\n",
    "biases = []\n",
    "variances = []\n",
    "mses = []\n",
    "for k in range(1, 20):\n",
    "\n",
    "    [bias,variance,mse] = cals(k)\n",
    "#     print bias, variance, mse\n",
    "    biases.append(bias)\n",
    "    variances.append(variance)\n",
    "    mses.append(mse)\n",
    "#     print k\n",
    "plt.plot(biases)\n",
    "plt.plot(variances)\n",
    "plt.plot(mses)\n",
    "plt.show()"
